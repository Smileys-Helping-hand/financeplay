{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "src/server.ts"
    },
    {
      "src": "/ai/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/reports/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/data/(.*)",
      "dest": "src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}
